# Production env template (PERSONALIZE_ANCHOR=tpm by default)
AOC_API_URL=https://api.ephapsys.ai
AOC_API_KEY=aoc_prod_replace_me
AGENT_TEMPLATE_ID=agent_prod_replace_me
PERSONALIZE_ANCHOR=tpm
EPHAPSYS_LOG_LEVEL=WARNING

# Optional HSM (Cloud KMS) settings when PERSONALIZE_ANCHOR=hsm
# HSM_KMS_KEY=projects/my-prod/locations/us-central1/keyRings/agents/cryptoKeys/agent-prod/cryptoKeyVersions/1
# HSM_KMS_ENDPOINT=privatekms.googleapis.com
# HSM_KMS_CREDENTIALS=/path/to/prod-service-account.json
# HSM_SLOT=projects/my-prod/locations/us-central1/keyRings/agents
# HSM_KEY_LABEL=agent-prod
# HSM_KMS_LOCATION=us-central1
# HSM_KMS_KEY_RING=agents
# HSM_KMS_KEY_NAME=helloworld
# HSM_KMS_USE_HSM=1
# COMPUTE_SERVICE_ACCOUNT=project-number-compute@developer.gserviceaccount.com
# Non-GCP deployments should configure HSM_HELPER instead of the KMS variables.
